<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.control.Label?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.HelloController">
        <Label fx:id="messageLabel" text="Hello, JavaFX!" />
        <Button text="Send message" onAction="#sendMessage"/>
        <ListView fx:id="messageView"/>
        <TextField fx:id="messageInput"/>
</VBox>
